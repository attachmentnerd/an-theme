{% comment %}
  Ultra Editable About Page Section
  Every aspect is customizable - text, images, colors, fonts, spacing, and more
  300+ customization options for complete control
{% endcomment %}

<style>
.about-page-ultra {
  /* Color Variables */
  --hero-gradient-start: {{ section.settings.hero_gradient_start }};
  --hero-gradient-end: {{ section.settings.hero_gradient_end }};
  --hero-text: {{ section.settings.hero_text_color }};
  --stat-number: {{ section.settings.stat_number_color }};
  --story-bg: {{ section.settings.story_bg_color }};
  --story-bg-alt: {{ section.settings.story_bg_alt_color }};
  --insight-bg: {{ section.settings.insight_bg_color }};
  --insight-border: {{ section.settings.insight_border_color }};
  --mission-bg: {{ section.settings.mission_bg_color }};
  --mission-text: {{ section.settings.mission_text_color }};
  --impact-card-bg: {{ section.settings.impact_card_bg }};
  --impact-border-1: {{ section.settings.impact_border_1 }};
  --impact-border-2: {{ section.settings.impact_border_2 }};
  --impact-border-3: {{ section.settings.impact_border_3 }};
  --impact-border-4: {{ section.settings.impact_border_4 }};
  --cta-gradient-start: {{ section.settings.cta_gradient_start }};
  --cta-gradient-end: {{ section.settings.cta_gradient_end }};
  --btn-primary-bg: {{ section.settings.btn_primary_bg }};
  --btn-primary-text: {{ section.settings.btn_primary_text }};
  --btn-secondary-border: {{ section.settings.btn_secondary_border }};
  --btn-secondary-text: {{ section.settings.btn_secondary_text }};
  
  /* Typography */
  font-family: -apple-system, BlinkMacSystemFont, sans-serif;
}

.about-page-ultra h1,
.about-page-ultra h2,
.about-page-ultra h3,
.about-page-ultra h4 {
  font-family: -apple-system, BlinkMacSystemFont, sans-serif;
  font-weight: {{ section.settings.heading_weight }};
}

.about-page-ultra .container {
  max-width: {{ section.settings.container_width }}px;
  margin: 0 auto;
  padding: 0 {{ section.settings.container_padding }}px;
}

/* Hero Section */
.about-page-ultra .hero {
  background: linear-gradient({{ section.settings.hero_gradient_angle }}deg, var(--hero-gradient-start) 0%, var(--hero-gradient-end) 100%);
  color: var(--hero-text);
  padding: {{ section.settings.hero_padding_top }}px 0 {{ section.settings.hero_padding_bottom }}px;
  position: relative;
  overflow: hidden;
}

{% if section.settings.hero_pattern %}
.about-page-ultra .hero::before {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: url('data:image/svg+xml,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 100"><circle fill="rgba(255,255,255,0.1)" cx="20" cy="20" r="2"/><circle fill="rgba(255,255,255,0.1)" cx="80" cy="40" r="1.5"/></svg>') repeat;
  opacity: {{ section.settings.hero_pattern_opacity }};
}
{% endif %}

.about-page-ultra .hero-content {
  position: relative;
  z-index: 1;
  text-align: {{ section.settings.hero_text_align }};
  max-width: {{ section.settings.hero_content_width }}px;
  margin: 0 auto;
}

.about-page-ultra .hero h1 {
  font-size: {{ section.settings.hero_title_size }}px;
  margin-bottom: {{ section.settings.hero_title_margin }}px;
  line-height: {{ section.settings.hero_title_line_height }};
  {% if section.settings.hero_title_uppercase %}text-transform: uppercase;{% endif %}
  {% assign hero_letter_spacing_value = section.settings.hero_title_letter_spacing | default: 0 | plus: 0 %}
  {% if hero_letter_spacing_value > 0 %}letter-spacing: {{ hero_letter_spacing_value }}px;{% endif %}
}

.about-page-ultra .tagline {
  font-size: {{ section.settings.tagline_size }}px;
  margin-bottom: {{ section.settings.tagline_margin }}px;
  opacity: {{ section.settings.tagline_opacity }};
  font-weight: {{ section.settings.tagline_weight }};
}

.about-page-ultra .hero-stats {
  display: flex;
  justify-content: {{ section.settings.stats_alignment }};
  gap: {{ section.settings.stats_gap }}px;
  margin: {{ section.settings.stats_margin_top }}px 0 {{ section.settings.stats_margin_bottom }}px;
  flex-wrap: wrap;
}

.about-page-ultra .stat {
  text-align: center;
}

.about-page-ultra .stat-number {
  font-size: {{ section.settings.stat_number_size }}px;
  font-weight: {{ section.settings.stat_number_weight }};
  display: block;
  margin-bottom: {{ section.settings.stat_number_margin }}px;
  color: var(--stat-number);
}

.about-page-ultra .stat-label {
  font-size: {{ section.settings.stat_label_size }}px;
  opacity: {{ section.settings.stat_label_opacity }};
  {% if section.settings.stat_label_uppercase %}text-transform: uppercase;{% endif %}
  {% assign stat_letter_spacing_value = section.settings.stat_label_letter_spacing | default: 0 | plus: 0 %}
  {% if stat_letter_spacing_value > 0 %}letter-spacing: {{ stat_letter_spacing_value }}px;{% endif %}
}

/* Story Sections */
.about-page-ultra .story-section {
  padding: {{ section.settings.story_padding_top }}px 0 {{ section.settings.story_padding_bottom }}px;
  background: var(--story-bg);
}

.about-page-ultra .story-section:nth-child(even) {
  background: var(--story-bg-alt);
}

.about-page-ultra .story-content {
  display: grid;
  grid-template-columns: {{ section.settings.story_icon_column_width }}fr {{ section.settings.story_text_column_width }}fr;
  gap: {{ section.settings.story_gap }}px;
  align-items: center;
  max-width: {{ section.settings.story_max_width }}px;
  margin: 0 auto;
}

{% if section.settings.story_alternate_layout %}
.about-page-ultra .story-section:nth-child(odd) .story-content {
  grid-template-columns: {{ section.settings.story_text_column_width }}fr {{ section.settings.story_icon_column_width }}fr;
}

.about-page-ultra .story-section:nth-child(odd) .story-visual {
  order: 2;
}
{% endif %}

.about-page-ultra .story-visual {
  text-align: center;
}

.about-page-ultra .story-icon {
  width: {{ section.settings.story_icon_size }}px;
  height: {{ section.settings.story_icon_size }}px;
  background: {{ section.settings.story_icon_bg }};
  border-radius: {{ section.settings.story_icon_border_radius }}%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: {{ section.settings.story_icon_emoji_size }}rem;
  margin: 0 auto {{ section.settings.story_icon_margin }}px;
  {% if section.settings.story_icon_shadow %}box-shadow: 0 {{ section.settings.story_icon_shadow_y }}px {{ section.settings.story_icon_shadow_blur }}px rgba(0,0,0,{{ section.settings.story_icon_shadow_opacity }});{% endif %}
}

.about-page-ultra .story-section h2 {
  font-size: {{ section.settings.story_heading_size }}px;
  color: {{ section.settings.story_heading_color }};
  margin-bottom: {{ section.settings.story_heading_margin }}px;
  line-height: {{ section.settings.story_heading_line_height }};
}

.about-page-ultra .story-section h3 {
  font-size: {{ section.settings.story_subheading_size }}px;
  color: {{ section.settings.story_subheading_color }};
  margin-bottom: {{ section.settings.story_subheading_margin }}px;
}

.about-page-ultra .story-text {
  font-size: {{ section.settings.story_text_size }}px;
  color: {{ section.settings.story_text_color }};
  line-height: {{ section.settings.story_text_line_height }};
  margin-bottom: {{ section.settings.story_text_margin }}px;
}

.about-page-ultra .story-highlight {
  color: {{ section.settings.story_highlight_color }};
  font-weight: {{ section.settings.story_highlight_weight }};
}

.about-page-ultra .insight-box {
  background: var(--insight-bg);
  padding: {{ section.settings.insight_padding }}px;
  border-radius: {{ section.settings.insight_border_radius }}px;
  border-left: {{ section.settings.insight_border_width }}px solid var(--insight-border);
  margin: {{ section.settings.insight_margin_top }}px 0 {{ section.settings.insight_margin_bottom }}px;
  {% if section.settings.insight_shadow %}box-shadow: 0 {{ section.settings.insight_shadow_y }}px {{ section.settings.insight_shadow_blur }}px rgba(0,0,0,{{ section.settings.insight_shadow_opacity }});{% endif %}
}

.about-page-ultra .insight-box h4 {
  color: {{ section.settings.insight_heading_color }};
  font-size: {{ section.settings.insight_heading_size }}px;
  font-weight: {{ section.settings.insight_heading_weight }};
  margin-bottom: {{ section.settings.insight_heading_margin }}px;
}

.about-page-ultra .insight-box p {
  color: {{ section.settings.insight_text_color }};
  font-size: {{ section.settings.insight_text_size }}px;
  line-height: {{ section.settings.insight_text_line_height }};
}

/* Mission Section */
.about-page-ultra .mission {
  background: var(--mission-bg);
  color: var(--mission-text);
  padding: {{ section.settings.mission_padding_top }}px 0 {{ section.settings.mission_padding_bottom }}px;
  text-align: center;
  position: relative;
  overflow: hidden;
}

{% if section.settings.mission_decoration %}
.about-page-ultra .mission::before {
  content: '';
  position: absolute;
  top: -50%;
  right: -25%;
  width: 600px;
  height: 600px;
  background: {{ section.settings.mission_decoration_color }};
  opacity: {{ section.settings.mission_decoration_opacity }};
  border-radius: 50%;
  filter: blur({{ section.settings.mission_decoration_blur }}px);
}
{% endif %}

.about-page-ultra .mission-content {
  max-width: {{ section.settings.mission_max_width }}px;
  margin: 0 auto;
  position: relative;
  z-index: 1;
}

.about-page-ultra .mission h2 {
  font-size: {{ section.settings.mission_heading_size }}px;
  margin-bottom: {{ section.settings.mission_heading_margin }}px;
}

.about-page-ultra .mission-quote {
  font-size: {{ section.settings.mission_quote_size }}px;
  font-style: {% if section.settings.mission_quote_italic %}italic{% else %}normal{% endif %};
  margin: {{ section.settings.mission_quote_margin_top }}px 0 {{ section.settings.mission_quote_margin_bottom }}px;
  opacity: {{ section.settings.mission_quote_opacity }};
  line-height: {{ section.settings.mission_quote_line_height }};
  padding: 0 {{ section.settings.mission_quote_padding }}px;
}

.about-page-ultra .mission-attribution {
  font-size: {{ section.settings.mission_attribution_size }}px;
  opacity: {{ section.settings.mission_attribution_opacity }};
  margin-top: {{ section.settings.mission_attribution_margin }}px;
  font-style: normal;
}

/* Impact Section */
.about-page-ultra .impact {
  padding: {{ section.settings.impact_padding_top }}px 0 {{ section.settings.impact_padding_bottom }}px;
  background: {{ section.settings.impact_bg_color }};
}

.about-page-ultra .impact h2 {
  text-align: center;
  font-size: {{ section.settings.impact_heading_size }}px;
  color: {{ section.settings.impact_heading_color }};
  margin-bottom: {{ section.settings.impact_heading_margin }}px;
}

.about-page-ultra .impact-grid {
  display: grid;
  grid-template-columns: repeat({{ section.settings.impact_columns }}, 1fr);
  gap: {{ section.settings.impact_gap }}px;
  max-width: {{ section.settings.impact_max_width }}px;
  margin: 0 auto;
}

.about-page-ultra .impact-card {
  background: var(--impact-card-bg);
  padding: {{ section.settings.impact_card_padding }}px;
  border-radius: {{ section.settings.impact_card_radius }}px;
  box-shadow: 0 {{ section.settings.impact_card_shadow_y }}px {{ section.settings.impact_card_shadow_blur }}px rgba(0,0,0,{{ section.settings.impact_card_shadow_opacity }});
  text-align: center;
  transition: all {{ section.settings.impact_card_transition }}s ease;
  position: relative;
  overflow: hidden;
}

.about-page-ultra .impact-card::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  height: {{ section.settings.impact_border_height }}px;
}

.about-page-ultra .impact-card:nth-child(1)::before {
  background: var(--impact-border-1);
}

.about-page-ultra .impact-card:nth-child(2)::before {
  background: var(--impact-border-2);
}

.about-page-ultra .impact-card:nth-child(3)::before {
  background: var(--impact-border-3);
}

.about-page-ultra .impact-card:nth-child(4)::before {
  background: var(--impact-border-4);
}

.about-page-ultra .impact-card:hover {
  transform: translateY(-{{ section.settings.impact_card_hover_lift }}px);
  box-shadow: 0 {{ section.settings.impact_card_hover_shadow_y }}px {{ section.settings.impact_card_hover_shadow_blur }}px rgba(0,0,0,{{ section.settings.impact_card_hover_shadow_opacity }});
}

.about-page-ultra .impact-icon {
  width: {{ section.settings.impact_icon_size }}px;
  height: {{ section.settings.impact_icon_size }}px;
  background: {{ section.settings.impact_icon_bg }};
  border-radius: {{ section.settings.impact_icon_radius }}%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: {{ section.settings.impact_icon_emoji_size }}rem;
  margin: 0 auto {{ section.settings.impact_icon_margin }}px;
}

.about-page-ultra .impact-card h3 {
  font-size: {{ section.settings.impact_card_heading_size }}px;
  color: {{ section.settings.impact_card_heading_color }};
  margin-bottom: {{ section.settings.impact_card_heading_margin }}px;
}

.about-page-ultra .impact-card p {
  color: {{ section.settings.impact_card_text_color }};
  font-size: {{ section.settings.impact_card_text_size }}px;
  line-height: {{ section.settings.impact_card_text_line_height }};
}

/* Final CTA */
.about-page-ultra .final-cta {
  background: linear-gradient({{ section.settings.cta_gradient_angle }}deg, var(--cta-gradient-start) 0%, var(--cta-gradient-end) 100%);
  color: {{ section.settings.cta_text_color }};
  padding: {{ section.settings.cta_padding_top }}px 0 {{ section.settings.cta_padding_bottom }}px;
  text-align: center;
  position: relative;
  overflow: hidden;
}

{% if section.settings.cta_pattern %}
.about-page-ultra .final-cta::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: url('data:image/svg+xml,<svg width="100" height="100" xmlns="http://www.w3.org/2000/svg"><circle cx="50" cy="50" r="40" fill="white" fill-opacity="0.05"/></svg>');
  background-size: {{ section.settings.cta_pattern_size }}px {{ section.settings.cta_pattern_size }}px;
}
{% endif %}

.about-page-ultra .final-cta-content {
  position: relative;
  z-index: 1;
}

.about-page-ultra .final-cta h2 {
  font-size: {{ section.settings.cta_heading_size }}px;
  margin-bottom: {{ section.settings.cta_heading_margin }}px;
}

.about-page-ultra .final-cta p {
  font-size: {{ section.settings.cta_text_size }}px;
  max-width: {{ section.settings.cta_text_max_width }}px;
  margin: 0 auto {{ section.settings.cta_text_margin }}px;
  line-height: {{ section.settings.cta_text_line_height }};
}

/* Buttons */
.about-page-ultra .btn-primary {
  background: var(--btn-primary-bg);
  color: var(--btn-primary-text);
  padding: {{ section.settings.btn_padding_y }}px {{ section.settings.btn_padding_x }}px;
  border: none;
  border-radius: {{ section.settings.btn_border_radius }}px;
  font-size: {{ section.settings.btn_font_size }}px;
  font-weight: {{ section.settings.btn_font_weight }};
  text-decoration: none;
  display: inline-block;
  margin: {{ section.settings.btn_margin }}px;
  transition: all {{ section.settings.btn_transition }}s ease;
  {% if section.settings.btn_shadow %}box-shadow: 0 {{ section.settings.btn_shadow_y }}px {{ section.settings.btn_shadow_blur }}px {{ section.settings.btn_shadow_color }};{% endif %}
}

.about-page-ultra .btn-primary:hover {
  transform: translateY(-{{ section.settings.btn_hover_lift }}px);
  {% if section.settings.btn_hover_shadow %}box-shadow: 0 {{ section.settings.btn_hover_shadow_y }}px {{ section.settings.btn_hover_shadow_blur }}px {{ section.settings.btn_hover_shadow_color }};{% endif %}
}

.about-page-ultra .btn-secondary {
  background: transparent;
  color: var(--btn-secondary-text);
  border: {{ section.settings.btn_secondary_border_width }}px solid var(--btn-secondary-border);
  padding: {{ section.settings.btn_padding_y | minus: 2 }}px {{ section.settings.btn_padding_x | minus: 2 }}px;
  border-radius: {{ section.settings.btn_border_radius }}px;
  font-size: {{ section.settings.btn_font_size }}px;
  font-weight: {{ section.settings.btn_font_weight }};
  text-decoration: none;
  display: inline-block;
  margin: {{ section.settings.btn_margin }}px;
  transition: all {{ section.settings.btn_transition }}s ease;
}

.about-page-ultra .btn-secondary:hover {
  background: var(--btn-secondary-border);
  color: {{ section.settings.btn_secondary_hover_text }};
}

/* Mobile Responsive */
@media (max-width: {{ section.settings.mobile_breakpoint }}px) {
  .about-page-ultra .hero h1 {
    font-size: {{ section.settings.hero_title_size_mobile }}px;
  }
  
  .about-page-ultra .hero-stats {
    gap: {{ section.settings.stats_gap_mobile }}px;
  }
  
  .about-page-ultra .story-content,
  .about-page-ultra .story-section:nth-child(odd) .story-content {
    grid-template-columns: 1fr;
    gap: {{ section.settings.story_gap_mobile }}px;
    text-align: center;
  }
  
  .about-page-ultra .story-section:nth-child(odd) .story-visual {
    order: 1;
  }
  
  .about-page-ultra .impact-grid {
    grid-template-columns: repeat({{ section.settings.impact_columns_mobile }}, 1fr);
  }
  
  .about-page-ultra .btn-primary,
  .about-page-ultra .btn-secondary {
    display: block;
    margin: {{ section.settings.btn_margin }}px auto;
    max-width: {{ section.settings.btn_max_width_mobile }}px;
  }
}
</style>

<div class="about-page-ultra">
  <!-- Hero Section -->
  <section id="{{ section.settings.section_id | default: 'about-page-ultra-editable' }}-{{ section.id }}" class="hero">
    <div class="container">
      <div class="hero-content">
        <h1>{{ section.settings.hero_title }}</h1>
        <p class="tagline">{{ section.settings.hero_tagline }}</p>
        
        {% if section.settings.show_stats %}
        <div class="hero-stats">
          <div class="stat">
            <span class="stat-number">{{ section.settings.stat_1_number }}</span>
            <div class="stat-label">{{ section.settings.stat_1_label }}</div>
          </div>
          <div class="stat">
            <span class="stat-number">{{ section.settings.stat_2_number }}</span>
            <div class="stat-label">{{ section.settings.stat_2_label }}</div>
          </div>
          <div class="stat">
            <span class="stat-number">{{ section.settings.stat_3_number }}</span>
            <div class="stat-label">{{ section.settings.stat_3_label }}</div>
          </div>
        </div>
        {% endif %}
        
        {% if section.settings.show_hero_cta %}
        <div class="hero-cta">
          {% if section.settings.hero_btn_1_text %}
          <a href="{{ section.settings.hero_btn_1_url }}" class="btn-primary">
            {{ section.settings.hero_btn_1_text }}
          </a>
          {% endif %}
          {% if section.settings.hero_btn_2_text %}
          <a href="{{ section.settings.hero_btn_2_url }}" class="btn-secondary">
            {{ section.settings.hero_btn_2_text }}
          </a>
          {% endif %}
        </div>
        {% endif %}
      </div>
    </div>
  </section>

  <!-- Story Section 1 -->
  {% if section.settings.show_story_1 %}
  <section class="story-section" id="story">
    <div class="container">
      <div class="story-content">
        <div class="story-visual">
          {% if section.settings.story_1_icon_type == 'emoji' %}
          <div class="story-icon">{{ section.settings.story_1_emoji }}</div>
          {% elsif section.settings.story_1_icon_image %}
          <div class="story-icon">
            {% assign image = section.settings.story_1_icon_image %}
            {% assign alt = section.settings.story_1_label %}
            {% assign sizes = '120px' %}
            {% assign aspect_ratio = '1/1' %}
            {% include 'responsive-image' %}
          </div>
          {% endif %}
          <h3>{{ section.settings.story_1_label }}</h3>
        </div>
        <div class="story-text-content">
          <h2>{{ section.settings.story_1_heading }}</h2>
          {% if section.settings.story_1_text_format == 'rich' %}
            {{ section.settings.story_1_rich_text }}
          {% else %}
            <p class="story-text">{{ section.settings.story_1_text_1 }}</p>
            {% if section.settings.story_1_text_2 %}
            <p class="story-text">{{ section.settings.story_1_text_2 }}</p>
            {% endif %}
          {% endif %}
          {% if section.settings.show_story_1_insight %}
          <div class="insight-box">
            <h4>{{ section.settings.story_1_insight_heading }}</h4>
            <p>{{ section.settings.story_1_insight_text }}</p>
          </div>
          {% endif %}
        </div>
      </div>
    </div>
  </section>
  {% endif %}

  <!-- Story Section 2 -->
  {% if section.settings.show_story_2 %}
  <section class="story-section">
    <div class="container">
      <div class="story-content">
        <div class="story-text-content">
          <h2>{{ section.settings.story_2_heading }}</h2>
          {% if section.settings.story_2_text_format == 'rich' %}
            {{ section.settings.story_2_rich_text }}
          {% else %}
            <p class="story-text">{{ section.settings.story_2_text_1 }}</p>
            {% if section.settings.story_2_text_2 %}
            <p class="story-text">{{ section.settings.story_2_text_2 }}</p>
            {% endif %}
          {% endif %}
          {% if section.settings.show_story_2_insight %}
          <div class="insight-box">
            <h4>{{ section.settings.story_2_insight_heading }}</h4>
            <p>{{ section.settings.story_2_insight_text }}</p>
          </div>
          {% endif %}
        </div>
        <div class="story-visual">
          {% if section.settings.story_2_icon_type == 'emoji' %}
          <div class="story-icon">{{ section.settings.story_2_emoji }}</div>
          {% elsif section.settings.story_2_icon_image %}
          <div class="story-icon">
            {% assign image = section.settings.story_2_icon_image %}
            {% assign alt = section.settings.story_2_label %}
            {% assign sizes = '120px' %}
            {% assign aspect_ratio = '1/1' %}
            {% include 'responsive-image' %}
          </div>
          {% endif %}
          <h3>{{ section.settings.story_2_label }}</h3>
        </div>
      </div>
    </div>
  </section>
  {% endif %}

  <!-- Story Section 3 -->
  {% if section.settings.show_story_3 %}
  <section class="story-section">
    <div class="container">
      <div class="story-content">
        <div class="story-visual">
          {% if section.settings.story_3_icon_type == 'emoji' %}
          <div class="story-icon">{{ section.settings.story_3_emoji }}</div>
          {% elsif section.settings.story_3_icon_image %}
          <div class="story-icon">
            {% assign image = section.settings.story_3_icon_image %}
            {% assign alt = section.settings.story_3_label %}
            {% assign sizes = '120px' %}
            {% assign aspect_ratio = '1/1' %}
            {% include 'responsive-image' %}
          </div>
          {% endif %}
          <h3>{{ section.settings.story_3_label }}</h3>
        </div>
        <div class="story-text-content">
          <h2>{{ section.settings.story_3_heading }}</h2>
