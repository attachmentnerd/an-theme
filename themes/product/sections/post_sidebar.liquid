{% if section.settings.show_sidebar %}
  <div class="section__sidebar col-lg-4">
    {% for block in section.blocks %}
      {% case block.type %}
        {% when 'post_info' %}
          {% include "post_info" %}
        {% when 'post_downloads' %}
          {% include "sidebar_downloads" %}
        {% when 'post_video_download' %}
          {% include "sidebar_video_download" %}
        {% when 'custom_sidebar_block' %}
          {% include "sidebar_custom" %}
        {% when 'instructor' %}
          {% include "sidebar_instructor" %}
        {% when 'sidebar_img' %}
          {% include "sidebar_img" %}
        {% when 'cross_sell' %}
          {% include "sidebar_cross_sell" %}
        {% when 'coaching_scheduling_widget' %}
          {% include "coaching_scheduling_widget", sectionType: 'sidebar' %}
        {% when 'course_progress' %}
          {% include "sidebar_course_progress" %}
        {% when 'continue_learning' %}
          {% include "sidebar_continue_learning" %}
        {% when 'lesson_navigation' %}
          {% include "sidebar_lesson_navigation" %}
        {% when 'module_navigation' %}
          {% include "sidebar_module_navigation" %}
      {% endcase %}
    {% endfor %}
  </div>
{% else %}
  <style>
    .section__body {
      width: 100%;
    }
  </style>
{% endif %}

