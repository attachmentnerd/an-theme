[
  {
    "name": "theme_info",
    "theme_name": "AN Product Theme",
    "theme_version": "AN-PROD 2025.09.28+25.1.0",
    "theme_author": "Kajabi",
    "theme_documentation_url": "https://themes.kajabi.com/changelog/product-themes/premier",
    "theme_support_url": "https://learn.kajabi.com",
    "settings_validatable": true
  },
  {
    "name": "Favicon",
    "elements": [
      {
        "type": "header",
        "content": "Favicon"
      },
      {
        "type": "image_picker",
        "id": "favicon",
        "label": "Favicon Image",
        "fit": "crop",
        "width": 32,
        "height": 32,
        "info": "Must be a 32x32 PNG file. Transparency is supported. Please allow some time for favicon to change."
      }
    ]
  },
  {
    "name": "Typography",
    "elements": [
      {
        "type": "header",
        "content": "Typography"
      },
      {
        "type": "divider"
      },
      {
        "type": "color",
        "id": "text_color",
        "label": "Dark Font Color",
        "default": "#000"
      },
      {
        "type": "color",
        "id": "text_color_light",
        "label": "Light Font Color",
        "default": "#fff"
      },
      {
        "type": "font_select",
        "label": "Base Font Family",
        "id": "font_family",
        "default": "Open Sans",
        "preset": "google_fonts",
        "options": []
      },
      {
        "type": "font_select",
        "label": "Heading Font Family",
        "id": "heading_font_family",
        "default": "Montserrat",
        "preset": "google_fonts",
        "options": []
      }
    ]
  },
  {
    "name": "Color Scheme",
    "elements": [
      {
        "type": "header",
        "content": "Color Scheme"
      },
      {
        "type": "color",
        "id": "color_primary",
        "label": "Primary",
        "default": "#2e91fc"
      },
      {
        "type": "color",
        "id": "color_offset",
        "label": "Offset",
        "default": "#f9f9f9"
      },
      {
        "type": "color",
        "id": "color_dark",
        "label": "Dark",
        "default": "#30373e"
      },
      {
        "type": "color",
        "id": "color_darker",
        "label": "Darker",
        "default": "#272d34"
      }
    ]
  },
  {
    "name": "Message Colors",
    "elements": [
      {
        "type": "header",
        "content": "Message Colors"
      },
      {
        "type": "color",
        "id": "color_success",
        "label": "Success",
        "default": "#1abc9c"
      },
      {
        "type": "color",
        "id": "color_info",
        "label": "Info",
        "default": "#34495e"
      },
      {
        "type": "color",
        "id": "color_warning",
        "label": "Warning",
        "default": "#f39c12"
      },
      {
        "type": "color",
        "id": "color_danger",
        "label": "Danger",
        "default": "#e74c3c"
      }
    ]
  },
  {
    "name": "General Appearance",
    "elements": [
      {
        "type": "header",
        "content": "General Appearance"
      },
      {
        "type": "color",
        "id": "color_container_bg",
        "label": "Background",
        "default": "#f9f9f9"
      },
      {
        "type": "checkbox",
        "id": "use_bg_img",
        "label": "Use Background Image",
        "default": "false"
      },
      {
        "type": "image_picker",
        "id": "page_background_img",
        "label": "Page Background Image",
        "info": "Suggested dimensions: 2500 × 2500"
      },
      {
        "type": "radio",
        "id": "v_align",
        "label": "Vertical Alignment",
        "default": "top",
        "options": [
          {
            "label": "Top",
            "value": "top"
          },
          {
            "label": "Middle",
            "value": "middle"
          }
        ]
      },
      {
        "type": "radio",
        "id": "body_width",
        "label": "Body Column Width",
        "default": "auto",
        "options": [
          {
            "label": "Auto",
            "value": "auto"
          },
          {
            "label": "Small",
            "value": "small"
          }
        ]
      }
    ]
  },
  {
    "name": "Navigation",
    "elements": [
      {
        "type": "header",
        "content": "Hardcoded Navigation"
      },
      {
        "type": "checkbox",
        "id": "nav_is_sticky",
        "label": "Sticky navigation",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "nav_show_login",
        "label": "Show login/avatar",
        "default": true
      },
      {
        "type": "text",
        "id": "nav_cta_label",
        "label": "CTA label",
        "default": "Get Started"
      },
      {
        "type": "text",
        "id": "nav_cta_url",
        "label": "CTA URL",
        "default": "/program/secure-parenting"
      },
      {
        "type": "select",
        "id": "nav_style",
        "label": "Nav style",
        "default": "solid",
        "options": [
          {
            "value": "solid",
            "label": "Solid (light background)"
          },
          {
            "value": "transparent",
            "label": "Transparent"
          }
        ]
      },
      {
        "type": "header",
        "content": "Logo"
      },
      {
        "type": "text",
        "id": "nav_logo_url",
        "label": "Logo URL (normal)",
        "default": "https://kajabi-storefronts-production.kajabi-cdn.com/kajabi-storefronts-production/file-uploads/themes/2162370086/settings_images/708217-6dc8-6e80-0466-7b518dfb74c7_6a4860b8-00bb-47fd-8078-c7f4dd4e5385.png"
      },
      {
        "type": "text",
        "id": "nav_logo_url_inverted",
        "label": "Logo URL (inverted)",
        "default": "https://kajabi-storefronts-production.kajabi-cdn.com/kajabi-storefronts-production//sites/2148666703/images/1d1d2ffe-9376-4248-b082-df05323d5e0b.webp"
      },
      {
        "type": "checkbox",
        "id": "nav_use_inverted_logo",
        "label": "Use inverted logo",
        "default": false
      }
    ]
  },
  {
    "name": "Custom Code",
    "elements": [
      {
        "type": "header",
        "content": "Custom Code"
      },
      {
        "type": "textarea",
        "id": "embedded_scripts",
        "label": "Embedded Scripts",
        "default": "<!-- Embedded '<script>' tags go here -->",
        "info": ""
      }
    ]
  },
  {
    "name": "Analytics & Tracking",
    "elements": [
      {
        "type": "header",
        "content": "Google Tag Manager"
      },
      {
        "type": "text",
        "id": "gtm_id",
        "label": "GTM Container ID",
        "info": "Enter your Google Tag Manager container ID (e.g., GTM-XXXXXX)"
      },
      {
        "type": "header",
        "content": "Custom Scripts"
      },
      {
        "type": "textarea",
        "id": "header_scripts",
        "label": "Header Scripts",
        "info": "Scripts to add before closing </head> tag. Do not include <script> tags."
      },
      {
        "type": "textarea",
        "id": "body_scripts",
        "label": "Body Scripts",
        "info": "Scripts to add before closing </body> tag. Do not include <script> tags."
      }
    ]
  }
]