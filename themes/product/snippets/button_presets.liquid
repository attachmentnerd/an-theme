<!--button-presets-start-->
<style class="button_presets">   

{% for i in (1..4) %}

    {%- assign preset_num =  "p" | append:i | append:"_" %}
    {%- assign bg_color_preset = 'bg_color_preset' | prepend:preset_num -%}
    {%- assign cta_bg_color = 'cta_bg_color' | prepend:preset_num -%}
    {%- assign cta_outline = 'cta_outline' | prepend:preset_num -%}
    {%- assign color_preset = 'color_preset' | prepend:preset_num -%}
    {%- assign cta_color = 'cta_color' | prepend:preset_num -%}
    {%- assign cta_min_width = 'cta_min_width' | prepend:preset_num -%}
    {%- assign cta_min_height = 'cta_min_height' | prepend:preset_num -%}
    {%- assign cta_font_size = 'cta_font_size' | prepend:preset_num -%}
    {%- assign cta_min_width_mobile = 'cta_min_width_mobile' | prepend:preset_num -%}
    {%- assign cta_min_height_mobile = 'cta_min_height_mobile' | prepend:preset_num -%}
    {%- assign cta_font_size_mobile = 'cta_font_size_mobile' | prepend:preset_num -%}
    {%- assign cta_border_color = 'cta_border_color' | prepend:preset_num -%}
    {%- assign cta_border_width = 'cta_border_width' | prepend:preset_num -%}
    {%- assign cta_border_radius = 'cta_border_radius' | prepend:preset_num -%}
    {%- assign cta_icon = 'cta_icon' | prepend:preset_num -%}
    {%- assign cta_hide_icon = 'cta_hide_icon' | prepend:preset_num -%}
    {%- assign icon_position = 'icon_position' | prepend:preset_num -%}
    {%- assign cta_style = 'cta_style' | prepend:preset_num -%}
    {%- assign v_bg_color_preset= settings[bg_color_preset] -%}
    {%- assign v_cta_bg_color= settings[cta_bg_color] -%}
    {%- assign v_cta_outline= settings[cta_outline] -%}
    {%- assign v_color_preset= settings[color_preset] -%}
    {%- assign v_cta_color = settings[cta_color] -%}
    {%- assign v_cta_min_width = settings[cta_min_width] -%}
    {%- assign v_cta_min_height = settings[cta_min_height] -%}
    {%- assign v_cta_font_size = settings[cta_font_size] -%}
    {%- assign v_cta_min_width_mobile  = settings[cta_min_width_mobile] -%}
    {%- assign v_cta_min_height_mobile = settings[cta_min_height_mobile] -%}
    {%- assign v_cta_font_size_mobile = settings[cta_font_size_mobile] -%}
    {%- assign v_cta_border_width = settings[cta_border_width] -%}
    {%- assign v_cta_border_radius = settings[cta_border_radius] -%}
    {%- assign v_cta_border_color = settings[cta_border_color] -%}
    {%- assign v_cta_icon = settings[cta_icon] -%}
    {%- assign v_cta_hide_icon  = settings[cta_hide_icon] -%} 
    {%- assign v_icon_position = settings[icon_position] -%}

    {%- assign style_code =  settings[cta_style] -%} 
    {%- include "style2vars" -%}
    {%- if v_cta_bg_color == blank -%} 
        {%- assign v_cta_bg_color = "var(" | append: v_bg_color_preset | append: ")" -%}
           {%- if v_bg_color_preset == "default" -%} 
                {%- assign v_cta_bg_color = v_bg_color_preset -%}
           {% endif %}

    {% endif %}

    {%- if v_cta_color == blank -%} 
        {%- assign v_cta_color = "var(" | append: v_color_preset | append: ")" -%}
           {%- if v_color_preset == "default" -%} 
                {%- assign v_cta_color = v_color_preset -%}
           {% endif %}
    {% endif %}


    .p{{i}}_btn{
        {%- unless v_cta_outline -%}
            {% if v_cta_bg_color != "default" %}
              background-color: {{v_cta_bg_color}}; 
            {%- endif -%}
        {%- else -%} 
            border: 2px solid {{v_cta_bg_color}};
            background-color: unset;
        {%- endunless -%}   

        {%- if v_cta_min_width != blank -%} 
            min-width: {{v_cta_min_width}}; 
        {%- endif -%} 
        {%- if v_cta_min_height != blank -%} 
            min-height: {{v_cta_min_height}}; 
        {%- endif -%} 

        {%- if v_cta_border_width != blank -%} 
                border-width:{{v_cta_border_width}};
        {%- endif -%} 
        {%- if v_cta_border_radius != blank -%} 
                border-radius:{{v_cta_border_radius}};
        {%- endif -%} 
        {%- if v_cta_border_color != blank -%} 
                border-color:{{v_cta_border_color}};
        {%- endif -%} 
        {{desktop_style}}
    } 
    .p{{i}}_btn span{
        {% if v_cta_color != "default" %}
             color:  {{v_cta_color}};
        {%- endif -%}
        {%- if v_cta_font_size != blank -%} 
            font-size:{{v_cta_font_size}};
        {%- endif -%} 
        {%- unless style_child == "" -%}
            {{style_child}}
        {%- endunless -%}
            {%- if button_text_style != "" -%} 
            {{button_text_style}}
        {%- endif -%} 
    }
    .p{{i}}_btn i{
        {% if v_cta_color != "default" %}
           color:  {{v_cta_color}};
        {%- endif -%} 
        {%- if v_cta_font_size != blank -%} 
            font-size:{{v_cta_font_size}};
        {%- endif -%} 
        {%- unless style_child == "" -%}
            {{style_child}}
        {%- endunless -%}
        {%- unless icon_child == "" -%}
            {{icon_child}} 
        {%- endunless -%}
    }



    {%- unless custom_selectors == "" -%}
       {{custom_selectors}}    
    {%- endunless -%}
    

 
        @media(max-width:786px){
        
        .p{{i}}_btn{
            {%- if v_cta_min_width_mobile != blank -%} 
                min-width: {{v_cta_min_width_mobile}}; 
            {%- endif -%} 
            {%- if v_cta_min_height_mobile != blank -%} 
                min-height: {{v_cta_min_height_mobile}}; 
            {%- endif -%} 
            {{mobile_style}}
        }
 
        .p{{i}}_btn span{
            {%- if v_cta_font_size_mobile != blank -%} 
                font-size:{{v_cta_font_size_mobile}};
            {%- endif -%} 
        }
 
        .p{{i}}_btn i{
            {%- if v_cta_font_size_mobile != blank -%} 
                font-size:{{v_cta_font_size_mobile}};
            {%- endif -%} 
        } 
 
        {%- unless custom_selectors_mobile == "" -%}
            {{custom_selectors_mobile}}    
        {%- endunless -%}
    }

      {%- unless tablet_style == "" and custom_selectors_tablet == ""-%} 
        @media(min-width:586px) and (max-width:786px){
            .p{{i}}_btn{
                {{tablet_style}}
            }
            {%- unless custom_selectors_tablet == "" -%}
              {{custom_selectors_tablet}}
            {%- endunless -%}
        }
     {%- endunless -%}
{% endfor %}
  
</style> 

    {%- assign style_code1111 =  settings.p1_cta_style -%} 
    {%- include "style2vars______" -%}

    {%- unless script == "" -%}
       <script>
         {{script}}  
       </script>
    {%- endunless -%}

<!--button-presets-end-->