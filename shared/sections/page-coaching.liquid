<div class="page-coaching">

    <!-- Hero Section -->
    <section class="hero">
        <div class="container">
            <div class="hero-content">
                <div class="hero-text">
                    <h1>{{ section.settings.h1_1_transform_your_paren }}</h1>
                    <p>Get personalized coaching to build secure, lasting bonds with your children using science-backed attachment strategies.</p>
                    <a href="{{ section.settings._btn_primary_1_book_your_session_url }}" class="btn-primary">{{ section.settings._btn_primary_1_book_your_session }}</a>
                </div>
                <div class="hero-image">
                    <!--@id=hero_coaching-->
                    <img src="{{ section.settings.img_1_parent_and_child_playing | image_picker_url: '2000x' }}" alt="Parent and child playing together happily" width="600" height="400">
                </div>
            </div>
        </div>
    </section>
    
    <!-- Services Section -->
    <section class="services">
        <div class="container">
            <h2>{{ section.settings.h2_not__h3__1_how_i_can_help_you }}</h2>
            <div class="services-grid">
                <div class="service-card">
                    <!--@id=service_1_icon-->
                    <img src="{{ section.settings.img_2_one_on_one_coaching_icon | image_picker_url: '2000x' }}" alt="One-on-one coaching icon" loading="lazy" width="80" height="80">
                    <h3>1:1 Coaching</h3>
                    <p>Personalized sessions tailored to your family's unique needs and challenges.</p>
                </div>
                <div class="service-card">
                    <!--@id=service_2_icon-->
                    <img src="{{ section.settings.img_3_group_coaching_icon | image_picker_url: '2000x' }}" alt="Group coaching icon" loading="lazy" width="80" height="80">
                    <h3>Group Programs</h3>
                    <p>Join a supportive community of parents on the same journey as you.</p>
                </div>
                <div class="service-card">
                    <!--@id=service_3_icon-->
                    <img src="{{ section.settings.img_4_emergency_support_icon | image_picker_url: '2000x' }}" alt="Emergency support icon" loading="lazy" width="80" height="80">
                    <h3>Crisis Support</h3>
                    <p>Get immediate guidance when you need it most with on-demand sessions.</p>
                </div>
            </div>
        </div>
    </section>
    
    <!-- Coach Section -->
    <section class="coach">
        <div class="container">
            <div class="coach-content">
                <div class="coach-image">
                    <!--@id=coach_photo-->
                    <img src="{{ section.settings.img_5_dr_sarah_mitchell_certif | image_picker_url: '2000x' }}" alt="Dr. Sarah Mitchell, Certified Parenting Coach" loading="lazy" width="400" height="500">
                </div>
                <div class="coach-text">
                    <h2>{{ section.settings.h2_not__h3__2_meet_your_coach }}</h2>
                    <p>Hi, I'm Dr. Sarah Mitchell. As a mother of three and certified attachment specialist, I understand the daily challenges of modern parenting.</p>
                    <p>My approach combines the latest neuroscience research with practical, real-world strategies that actually work in the chaos of family life.</p>
                    <p>I've helped over 500 families build stronger connections and create more peaceful homes. Let me help you transform your parenting journey too.</p>
                    <div class="credentials">
                        <span class="badge">Ph.D. Child Psychology</span>
                        <span class="badge">Certified Attachment Specialist</span>
                        <span class="badge">15+ Years Experience</span>
                    </div>
                </div>
            </div>
        </div>
    </section>
    
    <!-- CTA Section -->
    <section class="cta" id="book">
        <div class="container">
            <h2>{{ section.settings.h2_not__h3__3_ready_to_transform_y }}</h2>
            <p>Book a free 30-minute discovery call to see how coaching can help you.</p>
            <a href="{{ section.settings._btn_secondary_1_schedule_your_call_url }}" class="btn-secondary">{{ section.settings._btn_secondary_1_schedule_your_call }}</a>
        </div>
    </section>


</div>

